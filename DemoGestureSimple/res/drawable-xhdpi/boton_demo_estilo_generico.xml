<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
 
  <item android:state_enabled="false">
    <shape>
      <gradient android:startColor="#F7BF05" android:endColor="#F7BF05" android:angle="0" />
    </shape>
  </item>
 
  <item android:state_pressed="true">
    <shape>
      <gradient android:startColor="#f3d56c" android:endColor="#f3d56c" android:angle="0" />
    </shape>
  </item>
 
  <item android:state_focused="true">
    <shape>
      <gradient android:startColor="#F7BF05" android:endColor="#F7BF05" android:angle="0" />
    </shape>
  </item>
 
  <item>
    <shape>
      <gradient android:startColor="#F7BF05" android:endColor="#F7BF05" android:angle="0" />
    </shape>
  </item>
</selector>